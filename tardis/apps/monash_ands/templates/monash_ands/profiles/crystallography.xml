{% load xmldate %}<rif:registryObjects xmlns:rif="http://ands.org.au/standards/rif-cs/registryObjects">
   <rif:registryObject group="Monash University">
	<rif:key>MYTARDIS-CHANGEME-{{experiment.id}}<rif:/key>
	<rif:originatingSource>http://www.tardis.edu.au<rif:/originatingSource>
	<rif:collection type="dataset" dateAccessioned="{{experiment.update_time|toxmldatetime}}" dateModified="{{experiment.update_time|toxmldatetime}}">
		<rif:identifier type="local">experiment/view/{{experiment.id}}<rif:/identifier>
		<rif:identifier type="handle">hdl:{{experiment.handle}}<rif:/identifier>
		<rif:name type="primary">
			<rif:namePart>{{experiment.title}}<rif:/namePart>
		<rif:/name>
		<rif:location>
			<rif:address>
				<rif:electronic type="url">
					<rif:value>{{experiment.id}}<rif:/value>
					<rif:/electronic>
					<rif:physical>
						<rif:addressPart type="text">{{experiment.institution_name}}
						<rif:/addressPart>
					<rif:/physical>
			<rif:/address>
		<rif:/location>
		<rif:relatedObject>
			<rif:key>{{experiment.id}}<rif:/key>
			<rif:relation type="isOwnedBy">
				<rif:description>Is owned by<rif:/description>
				<rif:url>http://www.tardis.edu.au<rif:/url>
			<rif:/relation>
		<rif:/relatedObject>
		 {% for party_key in party_keys %}
		   <rif:relatedObject>
				<rif:key>{{party_key.party_param}}<rif:/key>
				<rif:relation type="{{party_key.relation_param}}"/>
			<rif:/relatedObject>
		{% endfor %}
		{% for activity_key in activity_keys %}
		   <rif:relatedObject>
				<rif:key>{{activity_key}}<rif:/key>
				<rif:relation type="isOutputOf"/>
			<rif:/relatedObject>
		{% endfor %}
		<rif:subject type="anzsrc-for" xml:lang="en">0601<rif:/subject>
		<rif:subject type="anzsrc-for" xml:lang="en">1101<rif:/subject>
		<rif:subject type="local" xml:lang="en">Macromolecular Crystallography<rif:/subject>
		<rif:subject type="local" xml:lang="en">Diffraction<rif:/subject>
		<rif:subject type="local" xml:lang="en">Protein Structure<rif:/subject>
		<rif:subject type="local" xml:lang="en">X-Ray<rif:/subject>
		<rif:subject type="local" xml:lang="en">Synchrotron<rif:/subject>
		<rif:description type="about">{{experiment.description}}<rif:/description>
    </rif:collection>
  </rif:registryObject>
</rif:registryObjects>
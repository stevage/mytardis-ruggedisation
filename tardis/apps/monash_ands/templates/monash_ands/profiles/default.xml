{% load xmldate %}<registryObject group="Monash University">
	<key>MYTARDIS-CHANGEME-{{experiment.id}}</key>
	<originatingSource>http://www.monash.edu.au</originatingSource>
	<collection type="dataset" dateAccessioned="{{experiment.update_time|toxmldatetime}}" dateModified="{{experiment.update_time|toxmldatetime}}">
		<identifier type="local">experiment/view/{{experiment.id}}</identifier>
		<identifier type="handle">hdl:{{experiment.handle}}</identifier>
		<name type="primary">
			<namePart>
			{{experiment.title}}
			</namePart>
		</name>
		<location>
			<address>
				<electronic type="url">
					<value>{{experiment.id}}</value>
				</electronic>
				<physical>
					<addressPart type="text">{{experiment.institution_name}}</addressPart>
				</physical>
			</address>
		</location>
		<relatedObject>
			<key>{{experiment.id}}</key>
			<relation type="isOwnedBy">
				<description>Is owned by</description>
				<url>http://www.monash.edu.au</url>
			</relation>
		</relatedObject>
		 {% for party_key in party_keys %}
		   <relatedObject>
				<key>{{party_key}}</key>
				<relation type="isPartOf"/>
			</relatedObject>
		{% endfor %}
		{% for activity_key in activity_keys %}
		   <relatedObject>
				<key>{{activity_key}}</key>
				<relation type="isPartOf"/>
			</relatedObject>
		{% endfor %}
    <description type="about">{% if custom_description %}{{custom_description}}{% else %}{{experiment.description}}{% endif %}</description>
  </collection>
</registryObject>
